    "product_example = df[df[0] == 1]\n",
    "\n",
    "# Làm mịn dữ liệu\n",
    "product_example[2] = product_example[2].rolling(window=14, min_periods=1).mean()\n",
    "\n",
    "# Chuyển đổi dữ liệu thành chuỗi thời gian\n",
    "sales_data = product_example[2].values\n",
    "\n",
    "# Chuẩn bị dữ liệu đầu vào cho LSTM\n",
    "def create_dataset(dataset, look_back=1):\n",
    "    dataX, dataY = [], []\n",
    "    for i in range(len(dataset) - look_back - 1):\n",
    "        a = dataset[i:(i + look_back)]\n",
    "        dataX.append(a)\n",
    "        dataY.append(dataset[i + look_back])\n",
    "    return np.array(dataX), np.array(dataY)\n",
    "\n",
    "# Chuẩn hóa dữ liệu\n",
    "scaler = MinMaxScaler(feature_range=(0, 1))\n",
    "sales_data_scaled = scaler.fit_transform(sales_data.reshape(-1, 1))\n",
    "\n",
    "# Tạo dataset lại với dữ liệu đã được scale\n",
    "look_back = 3\n",
    "x, y = create_dataset(sales_data_scaled.flatten(), look_back)\n",
    "\n",
    "# X_test = x\n",
    "# y_test = y\n",
    "train_size = int(len(x) * 0.67)\n",
    "X_train, X_test = x[:train_size], x[train_size:]\n",
    "y_train, y_test = y[:train_size], y[train_size:]\n",
    "\n",
    "X_test = X_test.reshape(X_test.shape[0], X_test.shape[1], 1)\n",
    "\n",
    "#--------------------------------------------\n",
    "# Dự đoán\n",
    "test_predict = model3.predict(X_test)\n",
    "\n",
    "# test_1_predict = model.predict(X_1_test)\n",
    "# test_2_predict = model.predict(X_2_test)\n",
    "\n",
    "# Tính toán độ chính xác hoặc lỗi của mô hình\n",
    "test_score = np.sqrt(mean_squared_error(y_test, test_predict[:,0]))\n",
    "print('Test Score: %.2f RMSE' % (test_score))\n",
    "\n",
    "# Use R-squared (R2) Score:\n",
    "r2 = r2_score(y_test, test_predict[:,0])\n",
    "print(f'R2 Test Score: {r2}')\n"
   ]
